{
  "name": "spotfire-wrapper",
  "version": "0.8.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-webplayer": "ng build spotfire-wrapper && (cd build/spotfire-wrapper/ ; npm pack)",
    "build-wrapper": "npm run build:elements",
    "test": "ng test",
    "lint": "ng lint",
    "builddev:elements": "ng build  && node build-script.js && wc elements/spotfire-wrapper.js",
    "build:elements": "ng build --prod && node build-script.js && wc elements/spotfire-wrapper.js",
    "sptf:watch": "(cd projects/spotfire-wrapper ; ../../node_modules/.bin/tsc -w  --pretty -p tsconfig.schematics.json)",
    "sptf:buildlib": "ng build spotfire-wrapper",
    "sptf:buildschematics": "(cd projects/spotfire-wrapper && npm run build)",
    "sptf:build": "npm run sptf:buildlib && npm run sptf:buildschematics",
    "build:lib": "npm run sptf:build && (cd build/spotfire-wrapper ; npm pack ; ls -l *tgz) && npm install --no-save build/spotfire-wrapper/*tgz && date"
  },
  "author": "Nicolas Deroche <nderoche@tibco.com> (http://www.tibco.com)",
  "private": false,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/elements": "^8.2.14",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@webcomponents/custom-elements": "*",
    "core-js": "^3.6.4",
    "rxjs": "^6.5.4",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.24",
    "@angular-devkit/core": "^8.3.25",
    "@angular-devkit/build-ng-packagr": "^0.801.0",
    "@angular/cli": "^8.3.24",
    "@angular/compiler-cli": "^8.2.14",
    "@types/jasmine": "^3.5.6",
    "@types/node": "^12.12.28",
    "codelyzer": "^5.2.1",
    "concat": "^1.0.3",
    "cpx": "^1.5.0",
    "fs-extra": "^8.1.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.1.0",
    "ts-node": "^8.6.2",
    "tsickle": "^0.35.0",
    "tslib": "^1.11.1",
    "tslint": "^5.20.1",
    "typescript": "<3.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TIBCOSoftware/spotfire-wrapper.git"
  },
  "homepage": "https://github.com/TIBCOSoftware/spotfire-wrapper",
  "description": "Wrapper for TIBCO Spotfire(R) library and package.",
  "bugs": {
    "url": "https://github.com/TIBCOSoftware/spotfire-wrapper/issues"
  },
  "main": "build-script.js",
  "keywords": [
    "TIBCO",
    "Spotfire"
  ],
  "license": "BSD-3-Clause"
}
