{
  "name": "spotfire-wrapper",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-webplayer": "ng build spotfire-webplayer && (cd dist/spotfire-webplayer/ ; npm pack)",
    "build-wrapper": "npm run build:elements",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "builddev:elements": "ng build  && node build-script.js && wc elements/spotfire-wrapper.js",
    "build:elements": "ng build --prod && node build-script.js && wc elements/spotfire-wrapper.js",
    "lib:build": "ng run zspotfire-webplayer:build",
    "lib:build:prod": "ng run zspotfire-webplayer:build:production",
    "lib:test": "ng run zspotfire-webplayer:test",
    "lib:lint": "ng run zspotfire-webplayer:lint",
    "schematics:build": "tsc -p projects/schematics/tsconfig.json",
    "schematics:copy": "npm run schematics:build && webpack",
    "build-package": "npm run lib:build && npm run schematics:copy"
  },
  "author": {
    "name": "Nicolas Deroche",
    "email": "nderoche@tibco.com",
    "url": "http://www.tibco.com"
  },
  "private": false,
  "dependencies": {
